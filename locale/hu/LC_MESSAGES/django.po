msgid "TF_VR_LeftRelief"
msgstr "Bal szemtávolság: %s1 mm (%s2 képpont)"

#: welcome.py:12 anotherfile.py:34
msgid "TF_VR_RightRelief"
msgstr "Jobb szemtávolság: %s1 mm (%s2 képpont)"

#: welcome.py:12 anotherfile.py:34
msgid "TF_VR_IpdStats"
msgstr "Pupillaközi távolság (IPD): %s1 mm (%s2 képpont)"

