msgid "TF_Training"
msgstr "KIKÉPZÉS"

#: welcome.py:12 anotherfile.py:34
msgid "TF_Training_Title"
msgstr "VÁLASSZ KIKÉPZÉSI MÓDOT"

#: welcome.py:12 anotherfile.py:34
msgid "TF_Training_Desc_BasicTraining"
msgstr "Tanuld meg egy osztály csínját-bínját."

#: welcome.py:12 anotherfile.py:34
msgid "TF_Training_Desc_OfflinePractice"
msgstr "Csiszold tudásod botokkal."

